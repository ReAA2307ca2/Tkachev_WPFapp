<Window x:Class="Tkachev_WPFapp.Windows.CreateNewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tkachev_WPFapp.Windows"
        mc:Ignorable="d"
        Title="CreateNewWindow" Height="450" Width="300">
    <StackPanel Margin="15">
        <Label FontSize="18">Start date</Label>
        <ComboBox ItemsSource="{Binding}"></ComboBox>

        <Label FontSize="18">Tech model</Label>
        <ComboBox ItemsSource="{Binding}"></ComboBox>

        <Label FontSize="18">Description</Label>
        <TextBox FontSize="18" Height="60"></TextBox>

        <Label FontSize="18">Start date</Label>
        <ComboBox ItemsSource="{Binding}"></ComboBox>
    </StackPanel>
</Window>
